<h1>Welcome to SoftUni Wiki!</h1>
<div class="spacer">
  <h2>{{title}}
    <div class="controls">
      <a href="/article/edit/{{_id}}">[edit]</a>
      <a href="/article/history/{{title}}">[history]</a>
    </div>
  </h2>
  <article>
    {{#each content}}
      <p>{{this}}</p>
    {{/each}}
  </article>
</div>